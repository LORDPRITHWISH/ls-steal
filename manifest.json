{
  "manifest_version": 3,
  "name": "LocalStorage Reader",
  "version": "1.1",
  "description": "Shows LocalStorage in popup",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
